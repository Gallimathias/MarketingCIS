<Window x:Class="TheMarketingPlatform.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TheMarketingPlatform"
        mc:Ignorable="d"
        Title="MainWindow" Height="413.2" Width="691.125" Foreground="#FF205081" Background="#FF205081" Icon="pack://siteoforigin:,,,/Speech-Bubble-Icons.ico" MinWidth="450" MinHeight="360">

    <Grid x:Name="MainGrid" Margin="0" Background="#FF205081">
        <Frame x:Name="MainFrame" Margin="146.91,10,10,10" NavigationUIVisibility="Hidden" Foreground="White" Background="#FFDDFFDD"/>
        <Menu x:Name="MainMenu" HorizontalAlignment="Left" Margin="10,10,0,10" Width="131.91" Background="#FF205081">
            <MenuItem x:Name="TicketsMenuItem" Header="Tickets" Height="30" Width="122" Margin="0,0,1,5" Foreground="White" Tag="Tickets"/>
            <MenuItem x:Name="SettingsMenuItem" Header="Settings" Height="30" Width="122" Margin="0,0,0,5" Foreground="White" Tag="Settings"/>
        </Menu>
    </Grid>
</Window>
